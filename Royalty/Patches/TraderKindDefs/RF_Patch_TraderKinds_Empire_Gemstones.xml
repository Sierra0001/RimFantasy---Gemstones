<?xml version="1.0" encoding="utf-8"?>
<Patch>

  <Operation Class="PatchOperationSequence">
    <success>Normal</success>
    <operations>

      <li Class="PatchOperationAdd">
        <xpath>*/TraderKindDef[defName = "Base_Empire_Standard"]/stockGenerators</xpath>
        <value>
          <li Class="StockGenerator_Category">
            <categoryDef>RF_RoughGemstones</categoryDef>
            <thingDefCountRange>0~8</thingDefCountRange>
            <countRange>0~3</countRange>
          </li>
          <li Class="StockGenerator_Category">
            <categoryDef>RF_Gemstones</categoryDef>
            <thingDefCountRange>0~5</thingDefCountRange>
            <countRange>0~3</countRange>
          </li>
        </value>
      </li>

      <li Class="PatchOperationAdd">
        <xpath>*/TraderKindDef[defName = "Empire_Caravan_TraderGeneral"]/stockGenerators</xpath>
        <value>
          <li Class="StockGenerator_Category">
            <categoryDef>RF_RoughGemstones</categoryDef>
            <thingDefCountRange>0~5</thingDefCountRange>
            <countRange>0~3</countRange>
          </li>
          <li Class="StockGenerator_Category">
            <categoryDef>RF_Gemstones</categoryDef>
            <thingDefCountRange>0~3</thingDefCountRange>
            <countRange>0~3</countRange>
          </li>
        </value>
      </li>

      <li Class="PatchOperationAdd">
        <xpath>*/TraderKindDef[defName = "Empire_Caravan_TributeCollector"]/stockGenerators</xpath>
        <value>
          <li Class="StockGenerator_BuyTradeTag">
            <tag>RF_RoughGemstones</tag>
          </li>
          <li Class="StockGenerator_BuyTradeTag">
            <tag>RF_Gemstones</tag>
          </li>
        </value>
      </li>

      <li Class="PatchOperationAdd">
        <xpath>*/TraderKindDef[defName = "Orbital_Empire"]/stockGenerators</xpath>
        <value>
          <li Class="StockGenerator_Category">
            <categoryDef>RF_RoughGemstones</categoryDef>
            <thingDefCountRange>0~8</thingDefCountRange>
            <countRange>0~3</countRange>
          </li>
          <li Class="StockGenerator_Category">
            <categoryDef>RF_Gemstones</categoryDef>
            <thingDefCountRange>0~5</thingDefCountRange>
            <countRange>0~3</countRange>
          </li>
        </value>
      </li>

    </operations>
  </Operation>

</Patch>
