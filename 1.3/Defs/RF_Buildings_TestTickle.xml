<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="BuildingBase">
    <defName>RF_TestTickleOutdoors</defName>
    <label>outdoor test tickle</label>
    <description>Looks like a thing placed on another thing.</description>
    <category>Building</category>
    <graphicData>
      <texPath>Things/Building/TestTickle/RF_TestTickle/RF_TestTickle</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawRotated>false</drawRotated>
      <drawSize>(1,1)</drawSize>
      <drawOffset>(0,0,0.10)</drawOffset>
      <allowFlip>true</allowFlip>
      <shadowData>
        <volume>(0.3, 0.5, 0.3)</volume> <!--This requires testing. -->
        <offset>(0,0,-0.23)</offset> <!--This requires testing. -->
      </shadowData>
      <damageData>
        <rect>(0.2,0,0.6,0.1)</rect> <!--This requires testing. -->
      </damageData>
    </graphicData>
    <rotatable>true</rotatable>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability> <!--Either Standable OR PassThroughOnly OR Impassable. -->
    <pathCost>30</pathCost>
    <!--<constructEffect>ConstructMetal</constructEffect> --> <!-- Not applicable to stuffable items.-->
    <tickerType>Normal</tickerType>
    <drawerType>RealtimeOnly</drawerType>
    <fillPercent>0.4</fillPercent>
    <statBases>
      <MaxHitPoints>320</MaxHitPoints>
      <Beauty>10</Beauty>
      <Mass>1</Mass>
      <WorkToBuild>360</WorkToBuild>
      <Flammability>0</Flammability>
    </statBases>
    <selectable>true</selectable>
    <constructionSkillPrerequisite>7</constructionSkillPrerequisite>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
    <costStuffCount>35</costStuffCount>
    <costList>
      <RF_RoughSapphire>45</RF_RoughSapphire>
    </costList>
    <building>
      <destroySound>BuildingDestroyed_Metal_Medium</destroySound>
    </building>
    <soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <resourcesFractionWhenDeconstructed>0.15</resourcesFractionWhenDeconstructed>
    <!--Are placeWorkers needed?-->
    <placeWorkers>
      <li>PlaceWorker_CoolerSimple</li>
    </placeWorkers>
    <comps>
      <li Class="CompProperties_Refuelable">
        <fuelConsumptionRate>10.0</fuelConsumptionRate>
        <fuelCapacity>20.0</fuelCapacity>
        <fuelFilter>
          <thingDefs>
            <li>WoodLog</li>
          </thingDefs>
        </fuelFilter>
        <initialFuelPercent>1</initialFuelPercent>
        <showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
      </li>
      <li Class="RimFantasy.CompProperties_Aura_Temperature">
        <auraRadius>5</auraRadius>
        <auraStrength>15</auraStrength>
        <locationMode>Outdoors</locationMode>
        <minTemperature>-20</minTemperature>
        <maxTemperature>20</maxTemperature>
        <workThroughWalls>true</workThroughWalls>
      </li>
      <li Class="RimFantasy.CompProperties_Aura_Food">
        <auraRadius>5</auraRadius>
        <auraStrength>0.02</auraStrength>
        <locationMode>Outdoors</locationMode>
        <minFood>0.1</minFood>
        <maxFood>0.9</maxFood>
        <workThroughWalls>true</workThroughWalls>
      </li>
      <li Class="RimFantasy.CompProperties_Aura_Joy">
        <auraRadius>5</auraRadius>
        <auraStrength>0.02</auraStrength>
        <locationMode>Outdoors</locationMode>
        <minJoy>0.1</minJoy>
        <maxJoy>0.9</maxJoy>
        <workThroughWalls>true</workThroughWalls>
      </li>
      <li Class="RimFantasy.CompProperties_Aura_Rest">
        <auraRadius>5</auraRadius>
        <auraStrength>0.02</auraStrength>
        <locationMode>Outdoors</locationMode>
        <minRest>0.1</minRest>
        <maxRest>0.9</maxRest>
        <workThroughWalls>true</workThroughWalls>
      </li>
      <li Class="RimFantasy.CompProperties_Aura_Mood">
        <auraRadius>5</auraRadius>
        <auraStrength>30</auraStrength>
        <locationMode>Outdoors</locationMode>
        <minMood>0.1</minMood>
        <maxMood>0.9</maxMood>
        <workThroughWalls>true</workThroughWalls>
      </li>
    </comps>
    <designationCategory>Furniture</designationCategory>
    <minifiedDef>MinifiedThing</minifiedDef>
    <thingCategories>
      <li>BuildingsFurniture</li>
    </thingCategories>
    <!--<researchPrerequisites>
      <li>RF_MagicalTemperatureRegulation</li>
    </researchPrerequisites> -->
  </ThingDef>

