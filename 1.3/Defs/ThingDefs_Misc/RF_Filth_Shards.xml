<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="BaseFilth">
    <defName>RF_RubbleDiamondShards</defName>
    <label>diamond shards</label>
    <statBases>
      <Beauty>-3</Beauty>
      <Cleanliness>-5</Cleanliness>
    </statBases>
    <graphicData>
      <texPath>Things/Filth/RF_RubbleDiamondShards</texPath>
      <!--<color>(0,0,0,0)</color>-->
    </graphicData>
    <filth>
      <disappearsInDays>45~50</disappearsInDays>
      <cleaningWorkToReduceThickness>35</cleaningWorkToReduceThickness>
      <cleaningSound>Interact_CleanFilth_Dirt</cleaningSound>
    </filth>
  </ThingDef>

