<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RecipeDef>
    <defName>RF_BulkCutDiamond</defName>
    <label>cut rough diamond x5</label>
    <description>Cut and polish a batch of rough diamonds into cut diamonds.</description>
    <jobString>Cutting diamond.</jobString>
    <workAmount>9000</workAmount> <!--10% faster than single cut-->
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <workSkill>Crafting</workSkill>
    <effectWorking>CutStone</effectWorking>
    <soundWorking>Recipe_MakeStoneBlocks</soundWorking>
  	<allowMixingIngredients>true</allowMixingIngredients>
		<targetCountAdjustment>1</targetCountAdjustment>
    <recipeUsers>
			<li>RF_MedievalGemCuttingBench</li>
			<!--<li>RF_IndustrialGemCuttingBench</li>-->
		</recipeUsers>
    <ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>RF_RoughDiamond</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>RF_RoughDiamond</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <RF_Diamond>5</RF_Diamond>
    </products>
    <skillRequirements>
        <Crafting>8</Crafting>
    </skillRequirements>
  </RecipeDef>

