<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="RockBase">
    <defName>RF_MineableRoughDiamond</defName>
    <label>rough diamond</label>
    <description>Rock containing bits of rough diamond.</description>
    <graphicData>
      <texPath>Things/Building/Linked/RockFlecked_Atlas</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <color>(109,109,109)</color>
      <colorTwo>(211,211,211)</colorTwo>
    </graphicData>
    <statBases>
      <MaxHitPoints>1750</MaxHitPoints>
    </statBases>
    <building>
      <isResourceRock>true</isResourceRock>
      <mineableThing>RF_RoughDiamond</mineableThing>
      <mineableYield>36</mineableYield>
      <mineableScatterCommonality>0.065</mineableScatterCommonality>
      <mineableScatterLumpSizeRange>2~8</mineableScatterLumpSizeRange>
    </building>
  </ThingDef>

